{
  "version": 2,
  "builds": [
    {
      "src": "./src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    "/:path#*"]
],
"rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.js" 
    }
  ],
"http": {
    "headers": [
      {
        "source": "/(.*)",
        "destination": "$1",
        "secure": false
      }
    ]
  },
"env": [
    {"name": "PORT", "value": "3000"}
  ]
}
